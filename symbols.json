{
    "OP": {
        "OP_AGRP": ["(", ")", "{", "}", "[", "]"],
        "OP_ASIG": ["=", "+=", "-=", "*=", "/="],
        "OP_ARIT": ["+", "-", "*", "/", "%", "++", "--"],
        "OP_RELA": ["<", ">", ">=", "<=", "==", "!="],
        "OP_LOGC": ["&&", "!", "||"],
        "OP_BITS": ["^", "~", "|", "&", "<<", ">>"],
        "OP_DELM": [";", ","],
        "OP": [".", "?", ":"]
    },

    "PR": {
        "PR_TYPE": ["int", "char", "string", "short", "bool", "void", "float", "double", "signed",
            "unsigned", "union", "enum", "false", "true"
        ],
        "PR_INST": ["return", "break", "case", "continue", "do", "while", "if",
            "else", "for", "goto", "typedef", "switch", "default"
        ],
        "PR_MACRO": ["#define", "#include", "#__line__", "#__file__",
            "#elif", "#if", "#endif", "#error", "#ifdef", "#ifndef",
            "#message", "#undef"
        ]
    }
}